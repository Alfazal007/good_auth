{
  "compilerOptions": {
    "target": "ES2020",
    "types": [],
    "module": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationDir": "./dist",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["src"]
}
